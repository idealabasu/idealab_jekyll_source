doi = {10.1109/ICRA.2014.6907045},
file = {:C$\backslash$:/Users/danaukes/Documents/Mendeley Desktop/An et al. - 2014 - An end-to-end approach to making self-folded 3D surface shapes by uniform heating.pdf:pdf},
isbn = {978-1-4799-3685-4},
issn = {10504729},
month = {may},
pages = {1466--1473},
publisher = {IEEE},
title = {{An end-to-end approach to making self-folded 3D surface shapes by uniform heating}},
