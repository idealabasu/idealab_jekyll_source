isbn = {978-0-7918-5181-4},
month = {aug},
pages = {V05BT07A068},
publisher = {American Society of Mechanical Engineers},
title = {{An Integrated Design and Simulation Environment for Rapid Prototyping of Laminate Robotic Mechanisms}},
