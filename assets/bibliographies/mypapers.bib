pages = {94671B},
title = {{PopupCAD: a tool for automated design, fabrication, and analysis of laminate devices}},
url = {http://proceedings.spiedigitallibrary.org/proceeding.aspx?doi=10.1117/12.2177576},
year = {2015}
}
